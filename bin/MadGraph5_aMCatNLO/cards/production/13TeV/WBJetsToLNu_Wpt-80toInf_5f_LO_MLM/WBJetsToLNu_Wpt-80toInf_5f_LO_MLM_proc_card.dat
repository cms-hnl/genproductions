import model sm-ckm_no_b_mass

define p = p b b~
define j = p
define bquark = b b~

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-

# Specify process(es) to run
generate p p > ell+ vl bquark / h @0
add process p p > ell+ vl bquark j / h @1
add process p p > ell+ vl bquark j j j / h @2
add process p p > ell+ vl bquark j j / h @3
add process p p > ell- vl~ bquark / h @4
add process p p > ell- vl~ bquark j / h @5
add process p p > ell- vl~ bquark j j / h @6
add process p p > ell- vl~ bquark j j j / h @7

# Output processes to MadEvent directory
output WBJetsToLNu_Wpt-80toInf_5f_LO_MLM -nojpeg
