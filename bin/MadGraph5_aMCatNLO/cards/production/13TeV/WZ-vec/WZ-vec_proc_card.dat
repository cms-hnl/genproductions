import model SMEFTsim_general_MwScheme_UFO-SMEFT_massless_private_vec
define p = g u c d s b u~ c~ d~ s~ b~
define j = p
define l+ = e+ mu+ ta+
define l- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
define lp = l+ l-
define vp = vl vl~

generate    p p > w+ z, w+ > lp vp, z > lp lp    NPprop=0 SMHLOOP=0 NP=1 @0
add process p p > w- z, w- > lp vp, z > lp lp    NPprop=0 SMHLOOP=0 NP=1 @1
add process p p > w+ z j, w+ > lp vp, z > lp lp  NPprop=0 SMHLOOP=0 NP=1 @2
add process p p > w- z j, w- > lp vp, z > lp lp  NPprop=0 SMHLOOP=0 NP=1 @3

output WZ-vec
