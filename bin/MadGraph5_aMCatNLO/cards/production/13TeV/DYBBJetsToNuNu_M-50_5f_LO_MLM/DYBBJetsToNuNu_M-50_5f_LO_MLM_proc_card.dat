import model sm-ckm_no_b_mass

define p = p b b~
define j = p
define bb = b b~

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-

# Specify process(es) to run
generate p p > vl vl~ bb bb / h @0
add process p p > vl vl~ bb bb j / h @1
add process p p > vl vl~ bb bb j j / h @2

# Output processes to MadEvent directory
output DYBBJetsToNuNu_M-50_5f_LO_MLM -nojpeg
