import model sm-ckm_lepton_masses_no_b_mass
#switch to diagonal ckm matrix if relevant for speed
#import model sm-lepton_masses_no_b_mass

define p = p b b~
define j = j b b~

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-
define ellpm = ell+ ell-
define vlvl~ = vl vl~

generate p p > ellpm vlvl~ @0
add process p p > ellpm vlvl~ j @1
add process p p > ellpm vlvl~ j j @2
add process p p > ellpm vlvl~ j j j @3
add process p p > ellpm vlvl~ j j j j @4

output wellnu01234j_5f_LO_MLM -nojpeg

