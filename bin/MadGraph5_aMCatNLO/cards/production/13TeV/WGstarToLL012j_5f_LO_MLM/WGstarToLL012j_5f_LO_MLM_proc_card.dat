#switch to diagonal ckm matrix if needed for speed
import model sm-lepton_masses_no_b_mass

define p = p b b~
define j = j b b~

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~

generate p p > ell- vl~ ell+ ell- @0
add process p p > ell+ vl ell+ ell- @1
add process p p > ell- vl~ ell+ ell- j @2
add process p p > ell+ vl ell+ ell- j @3
add process p p > ell- vl~ ell+ ell- j j @4
add process p p > ell+ vl ell+ ell- j j @5

output WGstarToLL012j_5f_LO_MLM -nojpeg
