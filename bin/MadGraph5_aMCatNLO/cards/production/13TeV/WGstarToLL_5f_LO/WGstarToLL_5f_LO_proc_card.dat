#switch to diagonal ckm matrix if needed for speed
import model sm-lepton_masses_no_b_mass

define p = p b b~
define j = j b b~

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~

generate p p > ell- vl~ e+ e- @0
add process p p > ell+ vl e+ e- @1
add process p p > ell- vl~ e+ e- j @2
add process p p > ell+ vl e+ e- j @3
add process p p > ell- vl~ e+ e- j j @4
add process p p > ell+ vl e+ e- j j @5
add process p p > ell- vl~ e+ e- j j j @6
add process p p > ell+ vl e+ e- j j j @7
add process p p > ell- vl~ e+ e- j j j j @8
add process p p > ell+ vl e+ e- j j j j @9

output WGstarToLL_5f_LO -nojpeg
