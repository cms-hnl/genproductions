set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_color_flows False
set gauge unitary
set complex_mass_scheme False
set max_npoint_for_channel 0
import model sm-ckm_no_b_mass
define l+ = e+ mu+
define l- = e- mu-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
define p = p b b~
define j = p
define bquark = b b~
define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-
generate p p > ell+ vl bquark / t h @0
add process p p > ell+ vl bquark j/ t h @1
add process p p > ell+ vl bquark j j/ t h @2
add process p p > ell+ vl bquark j j j/ t h @3
add process p p > ell- vl~ bquark/ t h @4
add process p p > ell- vl~ bquark j/ t h @5
add process p p > ell- vl~ bquark j j/ t h @6
add process p p > ell- vl~ bquark j j j/ t h @7
output WBJetsToLNu_Wpt-200toInf_5f_LO_MLM -nojpeg
